{{log 'product-fees-template' this}}
{{#each product.feePlans.data}}
    {{#if products}}
      {{#each products}}
      {{#if feePlanAssociation.association.rows}}
      {{> product-cohort-fees-template}}
      {{else}}
        {{#each feePlanAssociation.association.feePlanDetails}}
        {{> feeplan-template}}
        {{/each}}
      {{/if}}
      {{/each}}
    {{else if feePlans }}
        {{#each feePlans}}
        {{> feeplan-template}}
        {{/each}}
    {{else}}
      <b>No Fee Plans for this product</b>
    {{/if}}
  {{/each}}

